{
  "name": "elastic-circuit",
  "version": "0.3.0",
  "description": "elastic logic circuit tools",
  "main": "lib/",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "eslint lib/*.js test/*.js && mocha test",
    "templer": "./bin/templer.js --source rtl/ --output templates.js",
    "retest": "eslint lib/*.js && nyc mocha test",
    "redot": "for i in build/*.dot; do dot -Tsvg \"$i\" > \"$i\".svg; done",
    "clean": "rm build/*.dot rm build/*.svg build/*.v"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drom/elastic.git"
  },
  "keywords": [
    "elastic"
  ],
  "author": "Aliaksei Chapyzhenka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drom/elastic/issues"
  },
  "homepage": "https://github.com/drom/elastic#readme",
  "dependencies": {},
  "devDependencies": {
    "@drom/eslint-config": "^0.7.0",
    "chai": "^4.1.2",
    "escodegen": "^1.9.0",
    "eslint": "^4.8.0",
    "esprima": "^4.0.0",
    "estraverse": "^4.2.0",
    "fs-extra": "^5.0.0",
    "lodash.range": "^3.2.0",
    "lodash.template": "^4.4.0",
    "mocha": "^5.0.0",
    "nyc": "^11.2.1",
    "yargs": "^11.0.0"
  },
  "eslintConfig": {
    "extends": "@drom/eslint-config/eslint4/node4",
    "rules": {
      "no-console": 1
    }
  }
}
