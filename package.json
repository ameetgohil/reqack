{
  "name": "elastic",
  "version": "0.1.0",
  "description": "elastic logic tools",
  "main": "lib/",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "eslint lib/*.js test/*.js && mocha test",
    "retest": "eslint lib/*.js && nyc mocha test",
    "redot": "for i in *.dot; do dot -Tsvg \"$i\" > \"$i\".svg; done"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drom/elastic.git"
  },
  "keywords": [
    "elastic"
  ],
  "author": "Aliaksei Chapyzhenka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drom/elastic/issues"
  },
  "homepage": "https://github.com/drom/elastic#readme",
  "dependencies": {
    "digraph": "1.3.0"
  },
  "devDependencies": {
    "@drom/eslint-config": "^0.7.0",
    "chai": "^4.1.2",
    "escodegen": "^1.9.0",
    "eslint": "^4.8.0",
    "esprima": "^4.0.0",
    "estraverse": "^4.2.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1"
  },
  "eslintConfig": {
    "extends": "@drom/eslint-config/eslint4/node4",
    "rules": {
      "no-console": 1
    }
  }
}
